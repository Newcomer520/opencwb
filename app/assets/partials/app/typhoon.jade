section.typhoon(ng-controller='TyphoonCtrl')
    div(ng-repeat="marker in myMarkers",ui-map-marker="myMarkers[$index]")
    div(ng-repeat="circle in myCircles",ui-map-marker="myCircles[$index]")
    div(ng-repeat="path in myPaths",ui-map-marker="myPaths[$index]")
    div.map(ui-map="myMap",ui-options="mapOptions",ui-event="{'map-zoom_changed': 'setZoomMessage(myMap.getZoom())' }")
    div.legend
        div.CWB CWB {{CWBtime}}
        div.JTWC(ng-show='JTWCtime') JTWC {{JTWCtime}}Z

 
